@GtkWindow {
  @ChildProp("title", String)
  @InlineProp("id", String)
}

@GtkBox {
  @ChildProp("homogeneous", Bool)
  @ChildProp("visible", Bool)
}

@GtkLabel {
  @ChildArg("title", String)
  @ChildProp("visible", Bool)
  @ChildProp("align", String)
  @ChildProp("margin-top", Number)
  @ChildProp("margin-bottom", Number)
}
